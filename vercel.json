{
    "version": 2,
    "builds": [
        {
            "src": "twitter/wsgi.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "twitter/wsgi.py"
        }
    ],
    "build": {
        "env": {
            "DJANGO_SETTINGS_MODULE": "twitter.settings"
        },
        "command": "sh build_files.sh"
    }
}
